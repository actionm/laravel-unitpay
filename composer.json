{
    "name": "actionm/laravel-unitpay",
    "description": "UnitPay payments for Laravel",
    "keywords": [
        "laravel-unitpay",
        "unitpay",
        "payments"
    ],
    "homepage": "https://github.com/actionm/laravel-unitpay",
    "license": "MIT",
    "authors": [
        {
            "name": "actionm",
            "email": "actionmanager@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.4",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "ActionM\\UnitPay\\Facades\\": "src/Facades",
            "ActionM\\UnitPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActionM\\UnitPay\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
